{
  "publisher": "simonhe",
  "name": "export-what",
  "displayName": "export-what",
  "version": "0.0.31",
  "description": "export-what",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/export-what#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/export-what"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/export-what/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.77.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "properties": {
        "export-what.debug": {
          "type": "boolean",
          "default": false,
          "description": "Enable debug logging for the export-what extension"
        }
      }
    }
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "test": "vitest",
    "build": "tsdown src/index.ts --external vscode",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "pnpm lint --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@babel/parser": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@types/node": "^18.19.130",
    "@types/vscode": "1.77.0",
    "@vscode-use/utils": "0.1.39",
    "@vscode/vsce": "^3.7.0",
    "bumpp": "^9.11.1",
    "eslint": "^9.39.1",
    "fast-glob": "^3.3.3",
    "find-up": "^7.0.0",
    "lazy-js-utils": "^0.1.49",
    "tsdown": "^0.16.4",
    "typescript": "^5.9.3",
    "vitest": "^0.29.8"
  }
}
